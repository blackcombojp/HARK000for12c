    ' *　戻値　　　　　　：　なし
    ' *
    ' *-----------------------------------------------------------------------------/
    Private Sub Txt_Validated(ByVal sender As Object, ByVal e As EventArgs)

        Dim Tag As String

        Try

            Tag = CStr(CType(sender, GrapeCity.Win.Editors.GcTextBox).Tag)

            Select Case Tag

                Case "ID1" '部署コード

                    If IsNull(txt部署コード.Text.Trim) = False Then

                        If txt部署コード.Text.Trim.Length > 10 Then
                            MsgBox(MSG_501005, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Information, MsgBoxStyle), My.Application.Info.Title)
                            Initialize()
                            txt部署コード.Focus()
                            Exit Sub
                        End If

                        gintRtn = DLTP0501_PROC0102(xxxstrProgram_ID, xxxintSubProgram_ID, xxxintSPDSystemCode, xxxstr病院コード, txt部署コード.Text.Trim, gintSQLCODE, gstrSQLERRM)

                        Select Case gintRtn
                            Case 0
                                gblRtn = データ表示処理()
                                txt部署コード.Enabled = False
                                txt明細部署名.Focus()
                            Case 8
                                MsgBox(MSG_501006, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Information, MsgBoxStyle), My.Application.Info.Title)
                                Initialize()
                                txt部署コード.Focus()
                                Exit Sub
                            Case Else
                                MsgBox(gintSQLCODE & "-" & gstrSQLERRM, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Exclamation, MsgBoxStyle), My.Application.Info.Title)
                                Initialize()
                                txt部署コード.Focus()
                                Exit Sub

                        End Select

                    Else

                        txt部署コード.Focus()

                    End If

            End Select

        Catch ex As Exception

            log.Error(Set_ErrMSG(Err.Number, ex.ToString))
            MsgBox(MSG_COM902 & vbCr & Err.Number & vbCr & ex.Message, MsgBoxStyle.Critical Or MsgBoxStyle.OkOnly, My.Application.Info.Title)

        End Try

    End Sub


End Class