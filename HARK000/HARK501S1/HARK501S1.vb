    Private Sub Rb_CheckedChanged(sender As Object, e As EventArgs)

        Try

            If RB_更新.Checked = True Then

                txt部署コード.Text = ""
                txt部署コード.Enabled = True
                Initialize明細()
                txt明細部署コード.Enabled = False
                txt部署コード.Focus()

