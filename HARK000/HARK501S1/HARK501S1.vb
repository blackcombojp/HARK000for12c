    '/*-----------------------------------------------------------------------------
    ' *　モジュール機能　：　各種機能ボタン押下処理
    ' *
    ' *　注意、制限事項　：　なし
    ' *　引数１　　　　　：　sender・・オブジェクト識別クラス
    ' *　引数２　　　　　：　e・・イベントデータクラス
    ' *　戻値　　　　　　：　なし
    ' *-----------------------------------------------------------------------------/
    Private Sub Bt_ID_Click(ByVal sender As Object, ByVal e As EventArgs)

        Dim Tag As String
        Dim strSendFile As String = Nothing

        Try
            'Oracle接続状態確認
            If (OraConnectState(gintRtn, gintSQLCODE, gstrSQLERRM)) = False Then
                MsgBox(gintSQLCODE & "-" & gstrSQLERRM & vbCr & MSG_COM004 & vbCr & MSG_COM903, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Exclamation, MsgBoxStyle), My.Application.Info.Title)
                Exit Sub
            End If

            Tag = CStr(CType(sender, Button).Tag)

            Select Case Tag

                Case "ID1" 'なし
                Case "ID2" 'なし
                Case "ID3" 'クリア

                    gintRtn = MsgBox(MSG_COM001, CType(MsgBoxStyle.YesNo + MsgBoxStyle.DefaultButton2 + MsgBoxStyle.Question, MsgBoxStyle), My.Application.Info.Title)

                    If gintRtn = vbYes Then

                        RB_新規.Checked = True
                        Initialize()
                        txt部署コード.Focus()

                    End If

                    Exit Sub

                Case "ID4" 'ログ

                  '別ロジックで実装

                Case "ID5" '実行

                    If 実行前チェック処理() = False Then Exit Sub

                    gintRtn = MsgBox(MSG_101105, CType(MsgBoxStyle.YesNo + MsgBoxStyle.DefaultButton2 + MsgBoxStyle.Question, MsgBoxStyle), My.Application.Info.Title)

                    If gintRtn = vbNo Then

                        txt明細部署名.Focus()
                        Exit Sub

                    End If

                    gintRtn = DLTP0501_PROC0002(xxxstrProgram_ID, xxxintSubProgram_ID, xxxintSPDSystemCode, xxxstr病院コード, CInt(lblID.Text.Trim), xxxstr施設コード, txt明細部署コード.Text.Trim, txt明細部署名.Text.Trim, gintSQLCODE, gstrSQLERRM)

                    Select Case gintRtn
                        Case 0
                            MsgBox(MSG_501011, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Information, MsgBoxStyle), My.Application.Info.Title)
                        Case -54
                            MsgBox(MSG_501014, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Exclamation, MsgBoxStyle), My.Application.Info.Title)
                        Case Else
                            MsgBox(MSG_501012 & vbCr & gintSQLCODE & "-" & gstrSQLERRM, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Exclamation, MsgBoxStyle), My.Application.Info.Title)
                    End Select

                    RB_新規.Checked = True
                    Initialize()
                    txt部署コード.Focus()

                    Exit Sub

                Case "ID6" 'なし
                Case "ID7" 'なし
                Case "ID8" 'なし

            End Select

        Catch ex As Exception

            log.Error(Set_ErrMSG(Err.Number, ex.ToString))
            MsgBox(MSG_COM902 & vbCr & Err.Number & vbCr & ex.Message, MsgBoxStyle.Critical Or MsgBoxStyle.OkOnly, My.Application.Info.Title)

        Finally

            Cursor = Cursors.Default

        End Try

    End Sub
    '/*-----------------------------------------------------------------------------
    ' *　モジュール機能　：　処理実行前チェック処理
    ' *
    ' *　注意、制限事項　：　なし
    ' *　引数１　　　　　：　なし
    ' *　引数２　　　　　：　なし
    ' *　戻値　　　　　　：　True・・正常、False・・異常
    ' *-----------------------------------------------------------------------------/
    Private Function 実行前チェック処理() As Boolean

        Try

            実行前チェック処理 = False

            If RB_更新.Checked = True Then

                '部署コード
                If IsNull(txt部署コード.Text.Trim) = True Then
                    MsgBox(MSG_501007, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Information, MsgBoxStyle), My.Application.Info.Title)
                    txt部署コード.Focus()
                    Exit Function
                End If

                '施設
                If IsNull(cmb施設.Text.Trim) = True Then
                    MsgBox(MSG_501009, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Information, MsgBoxStyle), My.Application.Info.Title)
                    cmb施設.Focus()
                    Exit Function
                End If

                '部署名
                If IsNull(txt明細部署名.Text.Trim) = True Then
                    MsgBox(MSG_501008, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Information, MsgBoxStyle), My.Application.Info.Title)
                    txt明細部署名.Focus()
                    Exit Function
                End If

            Else

                '施設
                If IsNull(cmb施設.Text.Trim) = True Then
                    MsgBox(MSG_501009, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Information, MsgBoxStyle), My.Application.Info.Title)
                    cmb施設.Focus()
                    Exit Function
                End If

                '明細部署コード
                If IsNull(txt明細部署コード.Text.Trim) = True Then
                    MsgBox(MSG_501010, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Information, MsgBoxStyle), My.Application.Info.Title)
                    txt明細部署コード.Focus()
                    Exit Function
                End If

                '部署名
                If IsNull(txt明細部署名.Text.Trim) = True Then
                    MsgBox(MSG_501008, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Information, MsgBoxStyle), My.Application.Info.Title)
                    txt明細部署名.Focus()
                    Exit Function
                End If

                gintRtn = DLTP0501_PROC0102(xxxstrProgram_ID, xxxintSubProgram_ID, xxxintSPDSystemCode, xxxstr病院コード, txt明細部署コード.Text.Trim, gintSQLCODE, gstrSQLERRM)

                Select Case gintRtn
                    Case 0
                        MsgBox(MSG_501013, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Information, MsgBoxStyle), My.Application.Info.Title)
                        Initialize()
                        txt部署コード.Focus()
                        Exit Function
                    Case 8
                        Exit Select
                    Case Else
                        MsgBox(gintSQLCODE & "-" & gstrSQLERRM, CType(MsgBoxStyle.OkOnly + MsgBoxStyle.Exclamation, MsgBoxStyle), My.Application.Info.Title)
                        Initialize()
                        txt部署コード.Focus()
                        Exit Function
                End Select

            End If

            実行前チェック処理 = True

            Exit Function

        Catch ex As Exception

            log.Error(Set_ErrMSG(Err.Number, ex.ToString))
            Throw

        End Try

    End Function
